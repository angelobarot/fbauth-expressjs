{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },
    "rules": {
      "no-console": 0,
      "consistent-return": 0,
      "no-underscore-dangle": 0,
      "valid-jsdoc": ["error", {
        "requireReturn": false,
        "requireReturnType": false,
        "requireParamDescription": false,
        "requireReturnDescription": false
      }],
      "require-jsdoc": ["error", {
          "require": {
              "FunctionDeclaration": false,
              "MethodDefinition": false,
              "ClassDeclaration": false
          }
      }]
    }
  }
